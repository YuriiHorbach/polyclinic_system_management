<h1>All patients</h1>

<%= link_to "New patient", new_patient_path %>
<hr>
<%@patients.each do |patient|%>
  <p><%= link_to patient.full_name, patient %> |
  <%= link_to "Destroy", patient_path(patient),
                  method: :delete,
                  data: { confirm: "Are you sure?" } %>  | 
  <%= link_to "Edit", edit_patient_path(patient)%> </p>
<% end %>
<hr>
<%= link_to "New patient", new_patient_path %>
