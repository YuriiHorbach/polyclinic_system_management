<h1>All patients</h1>

<%@patients.each do |patient|%>
  <p><%= link_to patient.full_name, patient %> |
  <%= link_to "Destroy", patient_path(patient),
                  method: :delete,
                  data: { confirm: "Are you sure?" } %> </p>
<% end %>

<%= link_to "New patient", new_patient_path %>
